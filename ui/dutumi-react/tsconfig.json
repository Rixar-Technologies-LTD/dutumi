{
  "compilerOptions": {
  "target": "es5",
  "baseUrl": "src",
  "paths": {
      "css/*": ["css/*"],
      "pages/*": ["pages/*"],
      "navigation/*": ["navigation/*"],
      "components/*": ["components/*"],
      "assets/*": ["assets/*"],
      "utils/*": ["utils/*"],
      "types/*": ["types/*"],
      "interfaces/*": ["interfaces/*"],
      "ui/*": ["ui/*"],
      "service/*": ["service/*"],
      "state/*": ["state/*"],
      "templates/*": ["templates/*"]
  },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
